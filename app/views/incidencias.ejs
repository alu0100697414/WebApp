<% include _head.ejs %>
<body ng-controller="IncidenciasCtrl">
<% include _navbar.ejs %>

<main><div class="container">
    <div class="row">
        <br><br>
        <h2 class="center indigo-text"> Incidencias </h2>
        <h6 class="center indigo-text"> (Incidencias que se han producido desde los dispositivos de las víctimas.) </h6>
    </div>

    <div class="row">
        <div class="col m12 s12">
            <table class="responsive-table bordered">
                <thead>
                <tr>
                    <th data-field="nombre">Nombre</th>
                    <th data-field="numero">Número de teléfono</th>
                    <th data-field="Lugar">Fecha</th>
                    <th data-field="Fecha">Incidencia</th>
                </tr>
                </thead>
                <tbody>
                <tr ng-repeat="incidencia in incidencias">
                    <td>{{incidencia.name}}</td>
                    <td>{{incidencia.number}}</td>
                    <td>{{incidencia.time}}</td>
                    <td>{{incidencia.text_incidence}}</td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>

</div></main>
<% include _footer.ejs %>
</body>
</html>
