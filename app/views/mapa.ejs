<% include _head.ejs %>
<body ng-controller="MapaCtrl">
<% include _navbar.ejs %>

<main><div class="container">
  <div class="row">
    <br><br>
    <h2 class="center indigo-text"> Mapa </h2>
  </div>

  <div class="row">
    <div class="col m4 s4">
      <div class="collection" ng-repeat="camara in camaras">
        <a ng-click="showMap(camara._id,camara.latitude,camara.longitude,camara.latitude_aggressor,camara.longitude_aggressor)" href="" class="collection-item">{{camara.name}} &#13; (tel: {{camara.number}})</a>
      </div>
    </div>

    <div class="col m8 s8">
      <div id="map" style="width: 625px; height: 350px;"></div>
    </div>

    </div>
  </div></main>

